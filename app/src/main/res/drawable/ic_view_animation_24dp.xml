<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportHeight="40.0"
    android:viewportWidth="40.0">

    <path
        android:fillColor="#FF000000"
        android:pathData="M20,15
                   a5,6 -15 0,0 -9 2
                   C11,22 15,23 20,29
                   C25,23 29,22 29,17
                   a5,6 15 0,0 -9,-2">

    </path>

</vector>